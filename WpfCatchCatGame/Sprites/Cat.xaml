<local:Sprite x:Class="WpfCatchCatGame.Sprites.Cat"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:svgc="http://sharpvectors.codeplex.com/svgc/"
             xmlns:local="clr-namespace:WpfCatchCatGame.Sprites"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <svgc:SvgViewbox IsHitTestVisible="False" Height="{Binding Radius}" Width="{Binding Radius}"  Source="/Assets/Images/left/1.svg">
            <svgc:SvgViewbox.RenderTransform>
                <TranslateTransform Y="-5" />
            </svgc:SvgViewbox.RenderTransform>
        </svgc:SvgViewbox>
    </Grid>
</local:Sprite>
